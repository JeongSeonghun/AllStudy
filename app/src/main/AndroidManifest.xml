        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.jshstudy.allstudy.ui.MainActivity" />
        <activity android:name="com.jshstudy.allstudy.ui.IntroActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.jshstudy.allstudy.ui.LoginActivity" />
        <activity android:name="com.jshstudy.allstudy.ui.engstudy.QuantityAdjectivesActivity" />
        <activity android:name="com.jshstudy.allstudy.ui.engstudy.EngStudyActivity" />
        <activity android:name="com.jshstudy.allstudy.ui.DBStudyActivity"></activity>
    </application>

</manifest>